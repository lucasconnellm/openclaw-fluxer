{
  "name": "@lucasconnellm/fluxer",
  "version": "0.4.0",
  "description": "OpenClaw Fluxer channel plugin powered by @fluxerjs/core",
  "type": "module",
  "license": "MIT",
  "private": false,
  "files": [
    "index.ts",
    "openclaw.plugin.json",
    "src",
    "README.md",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasconnellm/openclaw-fluxer.git"
  },
  "dependencies": {
    "@fluxerjs/core": "^1.2.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@types/node": "^22.19.11",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "openclaw": "^2026.2.17",
    "release-please": "^17.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "prepare": "husky",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run typecheck && pnpm test",
    "commit": "cz",
    "commit:check": "commitlint --edit"
  },
  "openclaw": {
    "channel": {
      "id": "fluxer",
      "label": "Fluxer",
      "selectionLabel": "Fluxer (@fluxerjs/core)",
      "docsPath": "/channels/fluxer",
      "docsLabel": "fluxer",
      "blurb": "Fluxer channel integration powered by @fluxerjs/core.",
      "aliases": [
        "fx"
      ],
      "order": 75,
      "quickstartAllowFrom": true
    },
    "extensions": [
      "./index.ts"
    ],
    "install": {
      "npmSpec": "@lucasconnellm/fluxer",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.30.0",
  "keywords": [
    "openclaw",
    "plugin",
    "fluxer",
    "bot"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
